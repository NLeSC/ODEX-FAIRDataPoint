#+ summary: Get all access and download URLs of data, only applicable to distribution metadata.
#+ method: GET
#+ endpoint_in_url: True

PREFIX dcat: <http://www.w3.org/ns/dcat#>
#PREFIX bif: <http://www.openlinksw.com/schemas/bif#> # for Virtuoso

SELECT DISTINCT (?o AS ?DataURL)
WHERE {
    {?s dcat:accessURL ?o} UNION {?s dcat:downloadURL ?o}
}
